              <row>
                <entry/>
                <entry>Meet</entry>
                <entry>Skatting</entry>
                <entry>Ware afmeting</entry>
                <entry>Verskil</entry>
              </row>
              <row>
                <entry>a)</entry>
                <entry>die lengte van jou opvoeder </entry>
                <entry/>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>b)</entry>
                <entry>die breedte van jul klaskamer</entry>
                <entry/>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>c)</entry>
                <entry>die hoogte van jou bank / tafel</entry>
                <entry/>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>d)</entry>
                <entry>die totale lengte van die skryfbord in die klas</entry>
                <entry/>
